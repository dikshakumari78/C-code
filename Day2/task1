#include <iostream>
#include <string>

using namespace std;

class Student {
private:
    string name;
    int age;

public:
    // Default constructor
    Student() {
        name = "Unknown";
        age = 0;
    }

    // Method to display student details
    void display() {
        cout << "Student Name: " << name << endl;
        cout << "Student Age : " << age << endl;
    }
};

int main() {
    // Create a Student object using the default constructor
    Student student1;

    // Display student details
    student1.display();

    return 0;
}
