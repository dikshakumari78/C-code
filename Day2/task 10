#include <iostream>

using namespace std;

// Function to swap two integers using references
void swap(int& a, int& b) {
    int temp = a;
    a = b;
    b = temp;
}

int main() {
    int num1 = 10;
    int num2 = 20;

    cout << "Before swap:" << endl;
    cout << "num1 = " << num1 << ", num2 = " << num2 << endl;

    // Call the swap function with references
    swap(num1, num2);

    cout << "After swap:" << endl;
    cout << "num1 = " << num1 << ", num2 = " << num2 << endl;

    return 0;
}
