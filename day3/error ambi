#include <iostream>
using namespace std;

class A {
    public:
    void display()
    {
        std::cout<<"class A "<<std::endl;
    }
};
class B {
     public:
    void display()
    {
        std::cout<<"class B "<<std::endl;
    }
};
class C: public A, public B {
     
    void display()
    {
        display();
    }
};
